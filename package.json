{
  "name": "rest-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "webpack": "webpack",
    "buildx": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./npm-debug.log --copy-files",
    "rmdist": "rimraf dist/",
    "startx": "npm run build && node dist/app.js",
    "startx2": "export BABEL_DISABLE_CACHE=1 && babel-node app.js",
    "buildx2": "rimraf dist/ && webpack",
    "clean-dist": "rimraf dist/",
    "build-dist": "webpack",
    "startup": "pm2 startup -u zyper --hp /home/zyper",
    "unstartup": "pm2 unstartup",
    "save": "pm2 save",
    "start-pm2": "pm2 start dev.yml --env no-epg",
    "cert": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 3650 -nodes -subj '/CN=localhost'",
    "stop": "pm2 stop server",
    "log": "pm2 log",
    "kill": "pm2 kill",
    "pm2-v": "pm2 -v",
    "start-pm2-nd": "pm2 start dev.yml --env no-epg --no-daemon",
    "start": "export NODE_ENV=no-epg && export BABEL_DISABLE_CACHE=1 && node -r babel-register -r babel-polyfill app.js",
    "start-prod": "pm2 start prod.yml",
    "test": "jest",
    "test-w": "jest --watch",
    "test2": "jest --watch --runInBand",
    "test-ps": "jest --runInBand controllers/pubSub/pubSub.test.js",
    "test-auth": "jest --runInBand controllers/sec/auth.test.js",
    "test-users": "jest --runInBand controllers/sec/users.test.js",
    "test-roles": "jest --runInBand controllers/sec/roles.test.js",
    "test-auth-w": "jest --watch --runInBand controllers/sec/auth.test.js",
    "test-users-w": "jest --watch --runInBand controllers/sec/users.test.js",
    "test-roles-w": "jest --watch --runInBand controllers/sec/roles.test.js",
    "tar": "rm -f ../rest-server.tar.gz && tar -zcvf ../rest-server.tar.gz ../rest-server --exclude ../rest-server/node_modules --exclude ../rest-server/build --exclude ../rest-server/dist",
    "create-tables": "export BABEL_DISABLE_CACHE=1 && node -r babel-register -r babel-polyfill db/createTables.js",
    "seedDb": "export BABEL_DISABLE_CACHE=1 && node -r babel-register -r babel-polyfill db/seedDb.js",
    "drop-tables": "export BABEL_DISABLE_CACHE=1 && node -r babel-register -r babel-polyfill db/dropTables.js",
    "cpw": "export BABEL_DISABLE_CACHE=1 && node -r babel-register -r babel-polyfill db/changePassword.js",
    "docker-save": "sudo docker save -o ../restserver.tar restserver",
    "docker-save2": "sudo docker save restserver | gzip > ../docker/restserver.tar.gz",
    "docker-load": "sudo docker load -i ../restserver.tar",
    "docker-load2": "sudo gzip -cd ../docker/restserver.tar.gz | docker load < STDOUT",
    "docker-gz": "sudo gzip -c ../restserver.tar > ../docker/restserver.tar.gz",
    "docker-ugz": "sudo gzip -cd ../docker/restserver.tar.gz > ../restserver.tar",
    "docker-logs": "sudo docker logs restserver",
    "docker-build": "sudo docker build -t restserver .",
    "docker-run": "sudo docker run --name restserver -p 8080:8080 --net host -i -t restserver",
    "docker-sh": "sudo docker run --rm --name restserver -p 8080:8080 --net host -i -t restserver /bin/sh",
    "docker-drop-tables": "sudo docker run --name restserver -p 8080:8080 --net host -i -t restserver yarn drop-tables",
    "docker-create-tables": "sudo docker run --name restserver -p 8080:8080 --net host -i -t restserver yarn create-tables",
    "docker-cpw": "sudo docker exec -it restserver yarn cpw",
    "docker-run-0": "sudo docker run --name restserver -u root -p 8080:8080 -i -t restserver",
    "docker-restart": "sudo docker run --restart=always --name restserver -p 8080:8080 --net host restserver",
    "docker-ps": "sudo docker ps",
    "docker-attach": "sudo docker attach restserver",
    "docker-exec": "sudo docker exec -it restserver /bin/sh",
    "docker-rm": "sudo docker rm --force restserver",
    "docker-stop": "sudo docker stop restserver",
    "build-folder": "ls -al /net/zaz1/home/tftpboot/zyper"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__DEV__": true,
      "port": 8080,
      "server": "127.0.0.1"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "bookshelf": "^0.10.4",
    "bookshelf-secure-password": "^3.0.1",
    "co": "^4.4.0",
    "co-body": "^5.0.0",
    "co-monk": "^1.0.0",
    "co-views": "^0.3.0",
    "form-data": "^2.2.0",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^7.4.3",
    "knex": "^0.13.0",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-cors": "^0.0.16",
    "koa-jsonwebtoken": "^1.3.1",
    "koa-jwt": "^3.2.2",
    "koa-logger": "^3.0.1",
    "koa-onerror": "^3.1.0",
    "koa-route": "^3.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "monk": "^4.0.0",
    "mysql": "^2.14.1",
    "node-fetch": "^1.7.2",
    "pm2": "^2.8.0",
    "readline-sync": "^1.4.7",
    "swig": "^1.4.2",
    "when": "^3.7.8",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "deep-merge": "^1.0.0",
    "gulp": "^3.9.1",
    "jest": "^20.0.4",
    "mocha": "^3.1.1",
    "monkey-hot-loader": "^0.0.3",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "supertest": "^3.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^1.7.0"
  }
}
